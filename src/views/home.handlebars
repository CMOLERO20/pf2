<div>
  <h1>Lista de productos</h1>
  {{#each producto}}
    <p>{{this.title}}</p><a href="/api/carts/:cid/product/{{this._id}}"><button
      >Agregar el carrito</button></a>
  {{/each}}

  <div>
    <p>Pagina actual {{page}}</p>
  </div>

  {{#if hasPrevPage}}
    <p>Pagina previa {{prevPage}}</p>
    <a href="/api/views?page={{prevPage}}"><button
      >Anterior</button></a>
  {{/if}}

  {{#if hasNextPage}}
    <p>Pagina siguiente {{nextPage}}</p>
    <a href="/api/views?page={{nextPage}}"><button
      >Siguiente</button></a>
  {{/if}}
</div>